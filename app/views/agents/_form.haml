= form_for @agent_principal do |f|
  -if @agent_principal.errors.any?
    #error_explanation
      %h2= "#{pluralize(@agent_principal.errors.count, "error")} prohibited this project from being saved:"
      %ul
        - @agent_principal.errors.full_messages.each do |msg|
          %li= msg
  agent_id:
  = f.text_field :agent_id
  = f.text_field :start_date
  = f.text_field :end_date

  .actions
    = f.submit 'Save'
